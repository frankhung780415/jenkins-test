// 範例 Jenkinsfile
library 'test-lib'
pipeline {
    agent any // 不指定執行 agent
    stages { // 開始宣告 Pipeline 流程
        stage('franktest jenkins scm build') {
            // 行為宣告
            steps {
                echo 'Hello Github ~'
            }
        }
        stage('call lib method'){
            steps{
                call()
            }
        }
    }
}
